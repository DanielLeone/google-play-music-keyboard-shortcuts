{
    "manifest_version": 2,
    "name": "google play music keyboard shortcuts",
    "description": "keyboard shortcuts for google play music",
    "version": "1.0",
    "permissions": [
        "https://play.google.com/music/*",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "commands": {
        "prevSong": {
            "description": "previous song",
            "suggested_key": "Ctrl+Shift+1",
            "global": true
        },
        "playPause": {
            "description": "play / pause song",
            "suggested_key": "Ctrl+Shift+2",
            "global": true
        },
        "nextSong": {
            "description": "next song",
            "suggested_key": "Ctrl+Shift+3",
            "global": true
        }
    }
}
